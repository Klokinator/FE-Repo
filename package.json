{
  "name": "fire-emblem-graphics-repository",
  "version": "1.0.0",
  "description": "A repository for sharing fire emblem assets",
  "main": "index.js",
  "scripts": {
    "battle-animations-readme": "node ./repo-tools/battle-animations/make-battle-animations-readme.js && git add .",
